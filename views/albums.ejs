<%- include('partials/header') %>
    <section>
        <% spotifyAlbums.forEach(data=> { %>
            <article>
                <div>
                    <% if(data.images[0] !=undefined){%>
                        <img src="<%= data.images[0].url%> " alt="">
                        <%} else {%>
                            <img src="assets/img/Rectangle.png" alt="album cover">
                            <% } %>
                </div>
                <h3>
                    <%= data.name %>
                </h3>
                <a href="/albums/<%= data.id %>">View Albums</a>

                <!-- <img src="<%= data.images%> " alt=""> -->

                </div>
                <h3>
                    <%= data.name %>
                </h3>
                <a>View Albums</a>
            </article>
            <% }) %>
    </section>

    <%- include('partials/footer') %>